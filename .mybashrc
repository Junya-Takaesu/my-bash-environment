#-------------------------------------------------
# Load this '.mybashrc' from the real '~/.bashrc'
#-------------------------------------------------

DIR_NAME_MY_BASH_ENV=$HOME/my-bash-environment

# Load scripts and configs unique to this machine (git ignored)
. <(cat $DIR_NAME_MY_BASH_ENV/.custom-env/*)

# Load each setup scripts
. $DIR_NAME_MY_BASH_ENV/peco/setup.sh
. $DIR_NAME_MY_BASH_ENV/nvm/setup.sh
. $DIR_NAME_MY_BASH_ENV/cargo/setup.sh
. $DIR_NAME_MY_BASH_ENV/rbenv/setup.sh
. $DIR_NAME_MY_BASH_ENV/gh/setup.sh
. $DIR_NAME_MY_BASH_ENV/starship/setup.sh
. $DIR_NAME_MY_BASH_ENV/aws/setup.sh
. $DIR_NAME_MY_BASH_ENV/windows/setup.sh
. $DIR_NAME_MY_BASH_ENV/exa/setup.sh
. $DIR_NAME_MY_BASH_ENV/welcome/welcome.sh
